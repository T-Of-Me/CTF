{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug safe-paste",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "node",
      "runtimeArgs": [
        "--require",
        "tsx/cjs"
      ],
      "program": "${workspaceFolder}/2026/bits/safe-paste/server.ts",
      "cwd": "${workspaceFolder}/2026/bits/safe-paste",
      "sourceMaps": true,
      "resolveSourceMapLocations": [
        "${workspaceFolder}/2026/bits/safe-paste/**",
        "!**/node_modules/**"
      ],
      "env": {
        "NODE_ENV": "development"
      },
      "skipFiles": [
        "<node_internals>/**"
      ]
    }
  ]
}
